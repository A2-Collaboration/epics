{
	field( DESC, "Primary voltage setting")
	field( DTYP, "CAEN x527 generic HV Channel")
	field( OUT, "@$(CRATE).$(SLOT).$(CHANNEL) V0Set")
	field( PREC, "2")
	alias( $(NAME):v0set)
