{
	field( DTYP, "CAEN x527 generic HV Channel")
	field( INP, "@$(CRATE).$(SLOT).$(CHANNEL) ChName")
	field( SCAN, "Event")
	field( PINI, "1")
	field( VAL, "$(PSNAME):$(SLOT)")
}
