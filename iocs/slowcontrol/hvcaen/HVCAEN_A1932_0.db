record( stringout, "$(PSNAME):$(SLOT):$(CHANNEL):name")
{
	field( DTYP, "CAEN x527 generic HV Channel")
	field( OUT, "@$(CRATE).$(SLOT).$(CHANNEL) ChName")
}
record( stringout, "$(PSNAME):$(SLOT):$(CHANNEL):ename")
{
#        field( DTYP, "CAEN x527 generic HV Channel")
#        field( OUT, "@$(CRATE).$(SLOT).$(CHANNEL) ChName")
        field( VAL, $(NAME))
}

