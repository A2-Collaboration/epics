{
	field( OUT, "PS1014001:100:v0set:calc PP")
	field( PINI, "1")
	field( VAL, "0")
}
record( bo, "PS1014001:100:v0set:dn")
{
	field( OUT, "PS1014001:100:v0set:calc PP")
	field( PINI, "1")
	field( VAL, "0")
}
