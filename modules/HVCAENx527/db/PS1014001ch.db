record( ai, "PS1014001:011:ilock:in")
{
	field( INP, "PS1014001:011:vmon CPP")
	field( FLNK, "PS1014001:011:ilock:test")
}
record( mbbo, "PS1014001:011:ilock:cond")
{
	field( VAL, "0")
	field( ZRVL, "0")	field( ZRST, ">")
	field( ONVL, "1")	field( ONST, "=")
	field( TWVL, "2")	field( TWST, "<")
	field( THVL, "3")	field( THST, "!=")
	field( PINI, "1")
}
record( ao, "PS1014001:011:ilock:thresh")
{
	field( VAL, "10")
	field( PINI, "1")
}
# The calc record only determines if conditions have been met.
# The fanout actually determines if a signal should be sent.
record( calc, "PS1014001:011:ilock:test")
{
	field( INPA, "PS1014001:011:ilock:in")
	field( INPB, "PS1014001:011:ilock:thresh")
	field( INPJ, "PS1014001:011:ilock:cond")
	field( CALC, "J=1&&A=B||J=2&&A<B||J=0&&A>B||J=3&&A#B")
	field( FLNK, "PS1014001:011:ilock:act")
}
record( seq, "PS1014001:011:ilock:act")
{
	field( SELL, "PS1014001:011:ilock:test")
	field( SELM, "Specified")
	field( DOL1, "0")
	field( LNK1, "PS1014001:011:pwonoff PP")
}
